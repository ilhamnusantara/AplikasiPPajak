<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.perhitunganpajak2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="KlikPajak!"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PerhitunganPajak2">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PerhitunganPajak"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Pph21Golongan"
            android:parentActivityName=".PerhitunganPajak"/>
        <activity
            android:name=".InformasiPajak"
            android:parentActivityName=".MainActivity" />
    </application>

</manifest>