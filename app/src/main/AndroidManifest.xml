<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cekpajak">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="CekPajak!"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PerhitunganPajak2">
        <activity
            android:name="com.example.cekpajak.views.LoginActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.example.cekpajak.views.MainActivity"
            android:parentActivityName="com.example.cekpajak.views.LoginActivity" />
        <activity
            android:name="com.example.cekpajak.views.PerhitunganPajak"
            android:parentActivityName="com.example.cekpajak.views.MainActivity" />
        <activity
            android:name="com.example.cekpajak.views.Pph21Golongan"
            android:parentActivityName="com.example.cekpajak.views.PerhitunganPajak" />
        <activity
            android:name="com.example.cekpajak.views.PphFinal"
            android:parentActivityName="com.example.cekpajak.views.PerhitunganPajak" />
        <activity
            android:name="com.example.cekpajak.views.InformasiPajak"
            android:parentActivityName="com.example.cekpajak.views.MainActivity" />
    </application>


</manifest>